{
  "name": "gridiron-se-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "handlebars": "^4.0.14",
    "immutable": "^4.0.0-rc.9",
    "memoize-one": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^7.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "eslint src --fix && prettier --write 'src/**/**.js'",
    "css": "postcss --verbose src/App.css -o src/dist/App.css",
    "css:watch": "postcss --poll --verbose --watch src/App.css -o src/dist/App.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "plugin:flowtype/recommended"
    ],
    "env": {
      "jest": true,
      "es6": true,
      "browser": true
    },
    "rules": {
      "eqeqeq": [
        "error"
      ],
      "semi": [
        "error"
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6
    },
    "plugins": [
      "react"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-eslint": "^9.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.75.0",
    "postcss-cli": "^6.0.1",
    "prettier": "1.14.2",
    "serve": "^10.1.2",
    "tailwindcss": "^0.6.6"
  }
}
